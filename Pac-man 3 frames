int frame_1(int a, int b, int x) {
  u8g.setFont(u8g_font_unifont);
  u8g.drawBox(a+8*x,b,8*x,2*x);
  u8g.drawBox(a+4*x,b+2*x,12*x,2*x);
  u8g.drawBox(a+2*x,b+4*x,12*x,2*x);
  u8g.drawBox(a+2*x,b+6*x,10*x,2*x);
  u8g.drawBox(a,b+8*x,10*x,2*x);
  u8g.drawBox(a,b+10*x,8*x,2*x);
  u8g.drawBox(a,b+12*x,6*x,2*x);
  u8g.drawBox(a,b+14*x,8*x,2*x);
  u8g.drawBox(a,b+16*x,10*x,2*x);
  u8g.drawBox(a+2*x,b+18*x,10*x,2*x);
  u8g.drawBox(a+2*x,b+20*x,12*x,2*x);
  u8g.drawBox(a+4*x,b+22*x,12*x,2*x);
  u8g.drawBox(a+8*x,b+24*x,8*x,2*x);
}

int frame_2(int a, int b, int x) {
  u8g.setFont(u8g_font_unifont);
  u8g.drawBox(a+8*x,b,8*x,2*x);
  u8g.drawBox(a+4*x,b+2*x,16*x,2*x);
  u8g.drawBox(a+2*x,b+4*x,20*x,2*x);
  u8g.drawBox(a+2*x,b+6*x,20*x,2*x);
  u8g.drawBox(a,b+8*x,18*x,2*x);
  u8g.drawBox(a,b+10*x,12*x,2*x);
  u8g.drawBox(a,b+12*x,6*x,2*x);
  u8g.drawBox(a,b+14*x,12*x,2*x);
  u8g.drawBox(a,b+16*x,18*x,2*x);
  u8g.drawBox(a+2*x,b+18*x,20*x,2*x);
  u8g.drawBox(a+2*x,b+20*x,20*x,2*x);
  u8g.drawBox(a+4*x,b+22*x,16*x,2*x);
  u8g.drawBox(a+8*x,b+24*x,8*x,2*x);
}

int frame_3(int a, int b, int x) {
  u8g.setFont(u8g_font_unifont);
  u8g.drawBox(a+8*x,b,8*x,2*x);
  u8g.drawBox(a+4*x,b+2*x,16*x,2*x);
  u8g.drawBox(a+2*x,b+4*x,20*x,2*x);
  u8g.drawBox(a+2*x,b+6*x,20*x,2*x);
  u8g.drawBox(a,b+8*x,24*x,2*x);
  u8g.drawBox(a,b+10*x,24*x,2*x);
  u8g.drawBox(a,b+12*x,24*x,2*x);
  u8g.drawBox(a,b+14*x,24*x,2*x);
  u8g.drawBox(a,b+14*x,24*x,2*x);
  u8g.drawBox(a,b+16*x,24*x,2*x);
  u8g.drawBox(a+2*x,b+18*x,20*x,2*x);
  u8g.drawBox(a+2*x,b+20*x,20*x,2*x);
  u8g.drawBox(a+4*x,b+22*x,16*x,2*x);
  u8g.drawBox(a+8*x,b+24*x,8*x,2*x);
}
